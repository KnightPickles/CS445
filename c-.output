Grammar

    0 $accept: program $end

    1 program: declaration_list

    2 declaration_list: declaration_list declaration
    3                 | declaration

    4 declaration: var_declaration
    5            | fun_declaration

    6 var_declaration: type_specifier var_decl_list ';'

    7 scoped_var_declaration: scoped_type_specifier var_decl_list ';'

    8 var_decl_list: var_decl_list ',' var_decl_initialize
    9              | var_decl_initialize

   10 var_decl_initialize: var_decl_id
   11                    | var_decl_id ':' simple_expression

   12 var_decl_id: ID
   13            | ID '[' NUMCONST ']'

   14 scoped_type_specifier: STATIC type_specifier
   15                      | type_specifier

   16 type_specifier: INT
   17               | BOOL
   18               | CHAR

   19 fun_declaration: type_specifier ID '(' params ')' statement
   20                | ID '(' params ')' statement

   21 params: param_list
   22       | /* empty */

   23 param_list: param_list ';' param_type_list
   24           | param_type_list

   25 param_type_list: type_specifier param_id_list

   26 param_id_list: param_id_list ',' param_id
   27              | param_id

   28 param_id: ID
   29         | ID '[' ']'

   30 statement: unmatched
   31          | matched

   32 matched: matched_selection
   33        | matched_iteration
   34        | expression_stmt
   35        | compound_stmt
   36        | return_stmt
   37        | break_stmt

   38 unmatched: unmatched_selection
   39          | unmatched_iteration

   40 compound_stmt: '{' local_declarations statement_list '}'

   41 local_declarations: local_declarations scoped_var_declaration
   42                   | /* empty */

   43 statement_list: statement_list statement
   44               | /* empty */

   45 expression_stmt: expression ';'
   46                | ';'

   47 matched_selection: IF '(' simple_expression ')' matched ELSE matched

   48 unmatched_selection: IF '(' simple_expression ')' matched ELSE unmatched
   49                    | IF '(' simple_expression ')' statement

   50 matched_iteration: WHILE '(' simple_expression ')' matched
   51                  | FOREACH '(' mutable IN simple_expression ')' matched

   52 unmatched_iteration: WHILE '(' simple_expression ')' unmatched
   53                    | FOREACH '(' mutable IN simple_expression ')' unmatched

   54 return_stmt: RETURN ';'
   55            | RETURN expression ';'

   56 break_stmt: BREAK ';'

   57 expression: mutable '=' expression
   58           | mutable ADDASS expression
   59           | mutable SUBASS expression
   60           | mutable MULASS expression
   61           | mutable DIVASS expression
   62           | mutable INC
   63           | mutable DEC
   64           | simple_expression

   65 simple_expression: simple_expression '|' and_expression
   66                  | and_expression

   67 and_expression: and_expression '&' unary_rel_expression
   68               | unary_rel_expression

   69 unary_rel_expression: '!' unary_rel_expression
   70                     | rel_expression

   71 rel_expression: sum_expression relop sum_expression
   72               | sum_expression

   73 relop: LESSEQ
   74      | '<'
   75      | '>'
   76      | GRTEQ
   77      | EQ
   78      | NOTEQ

   79 sum_expression: sum_expression sumop term
   80               | term

   81 sumop: '+'
   82      | '-'

   83 term: term mulop unary_expression
   84     | unary_expression

   85 mulop: '*'
   86      | '/'
   87      | '%'

   88 unary_expression: unaryop unary_expression
   89                 | factor

   90 unaryop: '-'
   91        | '*'
   92        | '?'

   93 factor: immutable
   94       | mutable

   95 mutable: ID
   96        | ID '[' expression ']'

   97 immutable: '(' expression ')'
   98          | call
   99          | constant

  100 call: ID '(' args ')'

  101 args: arg_list
  102     | /* empty */

  103 arg_list: arg_list ',' expression
  104         | expression

  105 constant: NUMCONST
  106         | CHARCONST
  107         | STRINGCONST
  108         | BOOLCONST


